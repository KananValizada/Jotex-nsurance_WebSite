
@{
    ViewData["Title"] = "Index";
}

<!-- start page-title -->
<section class="page-title">
    <div class="page-title-container">
        <div class="page-title-wrapper">
            <div class="container">
                <div class="row">
                    <div class="col col-xs-12">
                        <h2>Haqqımızda</h2>
                        <ol class="breadcrumb">
                            <li><a asp-controller="home" asp-action="index">Home</a></li>
                            <li>Haqqımızda</li>
                        </ol>
                    </div>
                </div> <!-- end row -->
            </div> <!-- end container -->
        </div>
    </div>
</section>
<!-- end page-title -->
<section class="about-section-s4 section-padding">
    <div class="container">
      <vc:about-us></vc:about-us>
    </div> <!-- end container -->
</section>
<section class="fun-fact-section">
    <div class="container">
        <div class="row">
            <div class="col col-xs-12">
                <vc:fun-facts></vc:fun-facts>
            </div>
        </div>
    </div> <!-- end container -->
</section>

<!-- start agents-section -->
<section class="agents-section p-b-0 section-padding">
    <div class="container">
        <div class="row">
            <div class="col col-xs-12">
                <vc:agent></vc:agent>
            </div>
        </div>
    </div> <!-- end container -->
</section>
<!-- end agents-section -->
<!-- start testimonials-section-s2 -->
<vc:testimonial></vc:testimonial>
<!-- end testimonials-section-s2 -->
<!-- start partners-section -->
<section class="partners-section">
    <h2 class="hidden">Partners</h2>
    <div class="container">
        <div class="row">
            <div class="col col-xs-12">
                <div class="partner-grids partners-slider">
                    <div class="grid">
                        <img src="images/partners/img-1.png" alt="">
                    </div>
                    <div class="grid">
                        <img src="images/partners/img-2.png" alt="">
                    </div>
                    <div class="grid">
                        <img src="images/partners/img-3.png" alt="">
                    </div>
                    <div class="grid">
                        <img src="images/partners/img-4.png" alt="">
                    </div>
                    <div class="grid">
                        <img src="images/partners/img-5.png" alt="">
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- end container -->
</section>
<!-- end partners-section -->

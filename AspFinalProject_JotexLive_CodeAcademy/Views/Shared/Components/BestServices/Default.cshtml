@model IEnumerable<BestServicesViewModel>


    <div class="services-grids clearfix">
        @foreach (var item in Model)
        {
            <div class="grid">
                <a asp-controller="services" asp-action="index">
                    <i class="@item.Logo"></i>
                    <h3>@item.Name</h3>
                </a>
            </div>
        }

    </div>